{
    "version": "1.25.5",
    "description": "An open source programming language that makes it easy to build simple, reliable, and efficient software.",
    "homepage": "https://golang.org",
    "license": "BSD-3-Clause",
    "extract_dir": "go",
    "persist": ".gopath",
    "env_add_path": ".gopath\\bin",
    "env_set": {
        "GOROOT": "$dir",
        "GOPATH": "$persist_dir\\.gopath"
    },
    "bin": [
        "bin/go.exe",
        "bin/gofmt.exe"
    ],
    "architecture": {
        "64bit": {
            "url": "https://dl.google.com/go/go1.25.5.windows-amd64.zip",
            "hash": "ae756cce1cb80c819b4fe01b0353807178f532211b47f72d7fa77949de054ebb"
        },
        "32bit": {
            "url": "https://dl.google.com/go/go1.25.5.windows-386.zip",
            "hash": "a593393ea7715ffd315158f622a76226c3a4c4a0a6f92b1aeae03d7380cc06a3"
        }
    },
    "checkver": {
        "url": "https://golang.org/dl/",
        "regex": "go([\\d.]+)\\.windows-"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dl.google.com/go/go$version.windows-amd64.zip"
            },
            "32bit": {
                "url": "https://dl.google.com/go/go$version.windows-386.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    },
    "notes": [
        "Your GOROOT has been set to: $dir",
        "You can run 'go env GOROOT' to view this at any time."
    ]
}
